{
  "name": "archer-backend",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "watch": "webpack --watch",
    "start-squid": "npm-run-all --parallel start:local-core start:local-backend",
    "start:local-core": "node node_modules/@squidcloud/core/dist/main.js",
    "start:local-backend": "cross-env SQUID_REGION=local nest start --watch ./src --entryFile=backend/src/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch"
  },
  "author": "Squid Cloud",
  "license": "ISC",
  "dependencies": {
    "rxjs": "7.5.7",
    "@nestjs/common": "^9.0.0",
    "@squidcloud/common": "latest",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "@squidcloud/client": "latest",
    "@squidcloud/backend": "latest",
    "@supercharge/promise-pool": "^2.4.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@squidcloud/core": "latest",
    "@types/lodash": "^4.14.194",
    "copy-webpack-plugin": "^11.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "rimraf": "^3.0.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "zip-webpack-plugin": "^4.0.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "supertest": "^6.1.3",
    "cross-env": "^7.0.3",
    "mongodb-memory-server-core": "^8.11.5",
    "npm-run-all": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
