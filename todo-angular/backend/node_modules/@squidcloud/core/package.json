{
  "name": "@squidcloud/core",
  "version": "1.0.25",
  "description": "The Squid Cloud core service",
  "author": "",
  "main": "dist/main.js",
  "files": [
    "dist/main.js",
    "dist/package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "prebuild:local": "rimraf dist",
    "build": "nest build --webpack",
    "build:local": "cross-env IS_LOCAL_ENVIRONMENT=true nest build --webpack",
    "start:local": "cross-env OTEL_EXPORTER_OTLP_ENDPOINT=http://otel-collector:4318 nest start --watch ./src --watch ../internal-common/src --entryFile=core/src/main.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=prod node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "sandbox": "nodemon --watch \"src/sandbox.ts\" --exec \"ts-node -r tsconfig-paths/register src/sandbox.ts\"",
    "publish:public": "npm version patch && npm run build:local && npm publish --access public"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.272.0",
    "@aws-sdk/s3-request-presigner": "^3.272.0",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/platform-socket.io": "^9.0.11",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/websockets": "^9.0.11",
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/core": "^1.9.1",
    "@opentelemetry/resources": "^1.8.0",
    "@opentelemetry/semantic-conventions": "^1.8.0",
    "@redis/client": "^1.4.2",
    "@socket.io/redis-adapter": "^7.2.0",
    "@supercharge/promise-pool": "^2.3.2",
    "@supercharge/request-ip": "^1.2.0",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.2.1",
    "bson": "^4.7.0",
    "chalk": "^4.1.2",
    "cron-parser": "^4.6.0",
    "graphql-helix": "^1.13.0",
    "ioredis": "^5.2.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.4",
    "kafkajs": "^2.2.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.11.0",
    "mongodb-memory-server-core": "^8.11.5",
    "mssql": "^9.1.1",
    "mysql2": "^2.3.3",
    "nestjs-real-ip": "^2.2.0",
    "node-vault": "^0.9.22",
    "node-zookeeper-client": "^1.1.3",
    "openapi3-ts": "3.1.2",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "7.5.7",
    "socket.io": "^4.5.2",
    "swagger2openapi": "^7.0.8",
    "ts-deferred": "^1.0.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.0.0",
    "@types/decompress": "^4.2.4",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.10",
    "@types/lodash": "^4.14.185",
    "@types/mssql": "^8.1.2",
    "@types/multer": "^1.4.7",
    "@types/mysql": "^2.15.21",
    "@types/node": "^16.0.0",
    "@types/node-zookeeper-client": "^0.2.8",
    "@types/pg": "^8.6.6",
    "@types/prettyjson": "^0.0.30",
    "@types/supertest": "^2.0.11",
    "@types/unzip-stream": "^0.3.1",
    "cross-env": "^7.0.3",
    "json-schema-typed": "^8.0.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.1.3"
  }
}
